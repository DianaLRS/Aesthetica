<div class="wrapper">
    <div class="mypics">
        <div>
            {{> _sidenav }}
        </div>
        <div>

            {{!-- Importan UserID to be passed when favorite button is clicked --}}
            {{!--<h1 id="welcome" data-user-id="1">Welcome</h1>--}}
            <h1 class="display-3" id="welcome" data-user-id="1">My Closet</h1>
            <div class="row">

                {{#each photos}}

                <div class="col-sm-3">
                    <img src="{{imageURL}}" alt="" class="w-100" data-id="{{public_id}}">
                    <button type="button" class="btn btn-dark remove-from-closet">Remove</button>
                    <hr>
                    <div class="input-group">
                        <textarea class="form-control notes" aria-label="With textarea"></textarea>
                    </div>
                    <button type="button" class="btn btn-outline-secondary btn-sm add-note">Add Note</button>
                    {{!-- <button class="btn"><i class="fa fa-trash"></i></button> --}}
                </div>

                {{/each}}


            </div>

        </div>
    </div>

</div>